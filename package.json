{
  "name": "vue-mdbootstrap",
  "version": "2.2.0",
  "description": "Bootstrap5 Material Design Components for Vue.js",
  "author": {
    "name": "Ahmad Fajar",
    "email": "ahmad.fajar234@hotmail.com"
  },
  "license": "BSD-3-Clause",
  "keywords": [
    "bootstrap",
    "vue",
    "vue3",
    "vue-component",
    "vue-mdbootstrap"
  ],
  "homepage": "https://ahmadfajar.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmadfajar/vue-mdbootstrap"
  },
  "type": "module",
  "main": "dist/vue-mdb.esm.min.js",
  "module": "dist/vue-mdb.esm.mjs",
  "css-core": "dist/bundle.min.css",
  "css-components": "dist/bundle-colors.min.css",
  "css-colors": "dist/decorator-colors.min.css",
  "types": "dist/vue-mdb.d.ts",
  "unpkg": "dist/vue-mdb.umd.min.js",
  "jsdelivr": "dist/vue-mdb.umd.min.js",
  "files": [
    "dist",
    "scss",
    "component.d.ts",
    "package.json",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/vue-mdb.d.ts",
        "default": "./dist/vue-mdb.esm.mjs"
      },
      "require": {
        "types": "./dist/vue-mdb.d.ts",
        "default": "./dist/vue-mdb.cjs"
      }
    },
    "./component": "./component.d.ts",
    "./css-core": "./dist/bundle.css",
    "./css-components": "./dist/bundle-colors.css",
    "./css-colors": "./dist/decorator-colors.css",
    "./dist/*": "./dist/*",
    "./scss/*": "./scss/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "rimraf dist && run-p type-check build:dev css:compile css:color",
    "prod": "rimraf dist && run-p type-check build:prod css:compile css:color && run-s css:minify css:color-minify",
    "dist": "rimraf dist && run-p type-check build:dev build:prod build:lib build:types css:compile css:color && run-s css:minify css:color-minify",
    "build:dev": "vite build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:prod": "vite build --config vite.config.prod.ts",
    "build:types": "./node_modules/.bin/rollup --config rollup.config.dts.mjs",
    "css:compile": "sass --no-source-map src/bundle.scss dist/bundle.css",
    "css:beauty": "cleancss --format beautify -o dist/bundle.css dist/bundle.css",
    "css:minify": "cleancss -o dist/bundle.min.css dist/bundle.css",
    "css:color": "sass --no-source-map scss/custom/colors.scss dist/decorator-colors.css",
    "css:color-minify": "cleancss -o dist/decorator-colors.min.css dist/decorator-colors.css",
    "css:custom": "sass --no-source-map scss/custom/components.scss dist/bundle-custom.css",
    "css:custom-minify": "cleancss -o dist/bundle-custom.min.css dist/bundle-custom.css",
    "type-check": "tsc --noEmit -p tsconfig.build.json --composite false"
  },
  "peerDependencies": {
    "vue": ">= 3.5.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "fast-xml-parser": "^5.3.1",
    "lodash-es": "^4.17.21",
    "luxon": "^3.7.2",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@tsconfig/node24": "^24.0.1",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.0",
    "@types/sass": "^1.45.0",
    "@types/vue": "^2.0.0",
    "@vue/tsconfig": "^0.8.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "bootstrap": "^5.3.8",
    "clean-css-cli": "^5.6.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-sort-imports": "^1.8.8",
    "rimraf": "^6.1.0",
    "rollup-plugin-dts": "^6.2.3",
    "sass": "^1.93.3",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.1",
    "vue": "^3.5.23",
    "vue-router": "^4.6.3"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "packageManager": "yarn@4.10.3"
}
