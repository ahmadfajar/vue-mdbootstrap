{
  "name": "vue-mdbootstrap",
  "version": "2.0.15",
  "description": "Bootstrap5 Material Design Components for Vue.js",
  "author": {
    "name": "Ahmad Fajar",
    "email": "ahmad.fajar234@hotmail.com"
  },
  "license": "BSD-3-Clause",
  "keywords": [
    "bootstrap",
    "vue",
    "vue-component",
    "vue-mdbootstrap"
  ],
  "homepage": "https://ahmadfajar.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmadfajar/vue-mdbootstrap"
  },
  "type": "module",
  "main": "dist/vue-mdb.esm.min.js",
  "module": "dist/vue-mdb.esm.mjs",
  "styles": "dist/bundle.min.css",
  "types": "dist/vue-mdb.d.ts",
  "unpkg": "dist/vue-mdb.umd.min.js",
  "jsdelivr": "dist/vue-mdb.umd.min.js",
  "files": [
    "dist",
    "scss",
    "component.d.ts",
    "package.json",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/vue-mdb.d.ts",
        "default": "./dist/vue-mdb.esm.mjs"
      },
      "require": {
        "types": "./dist/vue-mdb.d.ts",
        "default": "./dist/vue-mdb.cjs"
      }
    },
    "./component": "./component.d.ts",
    "./styles": "./dist/bundle.css",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "rimraf dist && run-s type-check build:dev",
    "prod": "rimraf dist && run-s type-check build:prod",
    "dist": "rimraf dist && run-p type-check build:* css:bundle css:custom && run-s css:minify css:custom-minify",
    "build:dev": "vite build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:prod": "vite build --config vite.config.prod.ts",
    "build:types": "rollup --config rollup.config.dts.mjs",
    "css:bundle": "sass --no-source-map src/bundle.scss dist/bundle.css",
    "css:beauty": "cleancss --format beautify -o dist/bundle.css dist/bundle.css",
    "css:minify": "cleancss -o dist/bundle.min.css dist/bundle.css",
    "css:custom": "sass --no-source-map scss/custom/all.scss dist/custom-colors.css",
    "css:custom-minify": "cleancss -o dist/custom-colors.min.css dist/custom-colors.css",
    "type-check": "tsc --noEmit -p tsconfig.build.json --composite false"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "body-scroll-lock": "^4.0.0-beta.0",
    "fast-xml-parser": "^4.5.0",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/lodash": "^4.17.13",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.9.3",
    "@vue/tsconfig": "^0.6.0",
    "bootstrap": "5.2.3",
    "clean-css-cli": "^5.6.3",
    "compass-mixins": "^0.12.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup-plugin-dts": "^6.1.1",
    "sass": "^1.81.0",
    "terser": "^5.36.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vue-router": "^4.4.5"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ]
}
