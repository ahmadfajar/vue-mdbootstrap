{
  "name": "vue-mdbootstrap",
  "version": "2.0.0",
  "description": "Bootstrap5 Material Design Components for Vue.js",
  "author": {
    "name": "Ahmad Fajar",
    "email": "ahmad.fajar234@hotmail.com"
  },
  "license": "BSD-3-Clause",
  "type": "module",
  "main": "dist/vue-mdb.esm.min.js",
  "module": "dist/vue-mdb.esm.mjs",
  "styles": "dist/bundle.min.css",
  "types": "dist/vue-mdb.d.ts",
  "unpkg": "dist/vue-mdb.umd.min.js",
  "jsdelivr": "dist/vue-mdb.umd.min.js",
  "files": [
    "dist",
    "package.json",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/vue-mdb.d.ts",
        "default": "./dist/vue-mdb.esm.mjs"
      },
      "require": {
        "types": "./dist/vue-mdb.d.ts",
        "default": "./dist/vue-mdb.cjs"
      }
    },
    "./styles": "./dist/bundle.css",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "keywords": [
    "bootstrap",
    "vue",
    "vue-component",
    "vue-mdbootstrap"
  ],
  "homepage": "https://vue-mdbootstrap.fajarconsultant.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmadfajar/vue-mdbootstrap"
  },
  "scripts": {
    "build": "run-p type-check build-prod",
    "build:dist": "rimraf dist && run-p type-check build-dev build-prod build-lib build-types",
    "build-dev": "vite build",
    "build-lib": "vite build --config vite.config.lib.ts",
    "build-prod": "vite build --config vite.config.prod.ts",
    "build-types": "rollup --config rollup.config.dts.mjs",
    "type-check": "vue-tsc --noEmit -p tsconfig.build.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "bootstrap": "5.2.3",
    "compass-mixins": "^0.12.12",
    "fast-xml-parser": "^4.2.4",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.63.4",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node20": "^1.0.1",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/lodash": "^4.14.195",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.43.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-vue": "^9.15.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-dts": "^5.3.0",
    "terser": "^5.18.1",
    "typescript": "~5.1.3",
    "vite": "^4.3.9",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.8.1"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ]
}
