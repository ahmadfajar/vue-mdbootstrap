{
  "name": "vue-mdbootstrap",
  "version": "2.2.0",
  "description": "Bootstrap5 Material Design Components for Vue.js",
  "author": {
    "name": "Ahmad Fajar",
    "email": "ahmad.fajar234@hotmail.com"
  },
  "license": "BSD-3-Clause",
  "keywords": [
    "bootstrap",
    "vue",
    "vue3",
    "vue-component",
    "vue-mdbootstrap"
  ],
  "homepage": "https://ahmadfajar.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmadfajar/vue-mdbootstrap"
  },
  "type": "module",
  "main": "dist/vue-mdb.esm.min.js",
  "module": "dist/vue-mdb.esm.mjs",
  "css-core": "dist/bundle-core.min.css",
  "css-colors": "dist/decorator-colors.min.css",
  "theme-light": "dist/theme-light.min.css",
  "types": "dist/vue-mdb.d.ts",
  "unpkg": "dist/vue-mdb.umd.min.js",
  "jsdelivr": "dist/vue-mdb.umd.min.js",
  "files": [
    "dist",
    "scss",
    "component.d.ts",
    "package.json",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/vue-mdb.d.ts",
        "default": "./dist/vue-mdb.esm.mjs"
      },
      "require": {
        "types": "./dist/vue-mdb.d.ts",
        "default": "./dist/vue-mdb.cjs"
      }
    },
    "./component": "./component.d.ts",
    "./css-core": "./dist/bundle-core.min.css",
    "./css-colors": "./dist/decorator-colors.css",
    "./theme-light": "./dist/theme-light.css",
    "./dist/*": "./dist/*",
    "./scss/*": "./scss/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "rimraf dist && run-p type-check build:dev css:compile-core css:compile-light css:compile-color",
    "prod": "rimraf dist && run-p type-check build:prod css:compile-core css:compile-light css:compile-color && run-s css:minify-core css:minify-light css:minify-color",
    "dist": "rimraf dist && run-p type-check build:dev build:prod build:lib build:types css:compile-core css:compile-light css:compile-color && run-s css:minify-core css:minify-light css:minify-color",
    "build:dev": "vite build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:prod": "vite build --config vite.config.prod.ts",
    "build:types": "./node_modules/.bin/rollup --config rollup.config.dts.mjs",
    "css:compile-core": "sass --no-source-map src/bundle.scss dist/bundle-core.css",
    "css:beauty-core": "cleancss --format beautify -o dist/bundle-core.css dist/bundle-core.css",
    "css:minify-core": "cleancss -o dist/bundle-core.min.css dist/bundle-core.css",
    "css:compile-light": "sass --no-source-map scss/main/theme-light.scss dist/theme-light.css",
    "css:minify-light": "cleancss -o dist/theme-light.min.css dist/theme-light.css",
    "css:compile-color": "sass --no-source-map scss/custom/colors.scss dist/decorator-colors.css",
    "css:minify-color": "cleancss -o dist/decorator-colors.min.css dist/decorator-colors.css",
    "css:compile-custom": "sass --no-source-map scss/custom/components.scss dist/theme-custom.css",
    "css:minify-custom": "cleancss -o dist/theme-custom.min.css dist/theme-custom.css",
    "type-check": "tsc --noEmit -p tsconfig.build.json --composite false"
  },
  "peerDependencies": {
    "vue": ">= 3.5.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "fast-xml-parser": "^5.3.3",
    "lodash-es": "^4.17.21",
    "luxon": "^3.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@tsconfig/node24": "^24.0.3",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.3",
    "@types/sass": "^1.45.0",
    "@types/vue": "^2.0.0",
    "@vue/tsconfig": "^0.8.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "bootstrap": "^5.3.8",
    "clean-css-cli": "^5.6.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-sort-imports": "^1.8.9",
    "rimraf": "^6.1.2",
    "rollup-plugin-dts": "^6.3.0",
    "sass": "^1.97.0",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "packageManager": "yarn@4.12.0"
}
